{
  "name": "react-starter",
  "config": {
    "appname": "my-app-name"
  },
  "version": "6.0.1",
  "description": "lightweight react starterkit including webpack, unistore, styled components",
  "scripts": {
    "build": "BABEL_ENV=production webpack --config webpack/webpack.config.prod.js --colors && echo '/* /index.html  200' | cat >build/_redirects && echo 'User-Agent: * \nDisallow: ' | cat >build/robots.txt",
    "start": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "postbuild": "react-snap"
  },
