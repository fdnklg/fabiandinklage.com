{
  "name": "react-starter",
  "config": {
    "appname": "my-app-name"
  },
  "version": "6.0.1",
  "description": "lightweight react starterkit including webpack, unistore, styled components",
  "scripts": {
    "build": "cross-env AUTOPREFIXER_GRID=autoplace BABEL_ENV=production webpack --config webpack/webpack.config.prod.js --colors && echo '/* /index.html  200' | cat >build/_redirects && echo 'User-Agent: * \nDisallow: ' | cat >build/robots.txt",
    "start": "cross-env AUTOPREFIXER_GRID=autoplace webpack-dev-server --config webpack/webpack.config.dev.js",
    "postbuild": "react-snap",
    "sitemap": "babel-node src/sitemap/generator.js"
  },
  "reactSnap": {
    "inlineCss": false,
    "include": [
      "/projects/curricle/de",
      "/projects/curricle/en",
      "/projects/cityinflux/de",
      "/projects/cityinflux/en",
      "/projects/edutech/de",
      "/projects/edutech/en",
      "/projects/gender-equality/de",
      "/projects/gender-equality/en",
      "/projects/bikesharing/de",
      "/projects/bikesharing/de",
      "/projects/rhythmoftheroad/en",
      "/projects/rhythmoftheroad/en",
      "/projects/hackernews/en",
      "/projects/hackernews/en",
      "/projects/invisties/en",
      "/projects/invisties/en",
      "/projects/candidates/en",
      "/projects/candidates/en",
      "/home/de",
      "/home/en",
      "/profile/de",
      "/profile/en",
      "/contact/de",
      "/contact/en",
      "/legal/de",
      "/legal/en",
      "/home/en"
    ]
  },
  "repository": {
    "url": "git@github.com:wbkd/react-starter.git",
    "type": "git"
  },
  "author": "webkid.io",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-router-sitemap": "^1.2.0",
    "react-svg-loader": "^3.0.3",
    "style-loader": "^1.1.1",
    "webpack": "^4.41.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "@rebass/preset": "^4.0.5",
    "axios": "^0.19.2",
    "core-js": "^3.6.0",
    "easy-peasy": "^3.2.3",
    "es6-promise-promise": "^1.0.0",
    "history": "^4.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-lazyload": "^2.6.5",
    "react-markdown": "^4.2.2",
    "react-progressive-image": "^0.6.0",
    "react-remarkable": "^1.1.3",
    "react-router-dom": "^5.1.2",
    "react-router-i18n": "^0.3.0",
    "react-snap": "^1.23.0",
    "react-transition-group": "^4.3.0",
    "rebass": "^4.0.7",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6"
  }
}
